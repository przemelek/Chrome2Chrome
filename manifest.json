{
  "name": "Chrome2Chrome",
  "version": "0.6.1",
  "description": "Send page from Chrome to Chrom on another machine",
  "browser_action": {
    "popup": "chrome2chrome.html",
    "default_icon": "chrome2chrome.png"
  },  
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}